#endif

//-------
// SECS Info
//-------
#ifndef SECSInfo
typedef struct SECSInfo
{
	AnsiString SECS_TYPE;
	AnsiString SECS_LIP;
	AnsiString SECS_LPORT;
	AnsiString SECS_RIP;
	AnsiString SECS_RPORT;
	AnsiString SECS_T3;
	AnsiString SECS_T5;
	AnsiString SECS_T6;
	AnsiString SECS_T7;
	AnsiString SECS_T8;
	AnsiString SECS_T9;

	AnsiString SECS_COL;
	AnsiString SECS_ROW;

	AnsiString SECS_DEV_NUM;
	AnsiString SECS_MODELNAME;
	AnsiString SECS_VERSION;
}stSECSInfo;
#define SECSInfo
#endif

#ifndef MCS_COMMAND
typedef enum MCS_COMMAND
{
	MCS_COMMAND_ABORT = 1,
	MCS_COMMAND_CANCEL = 2,
	MCS_COMMAND_TRANSFER = 3,
	MCS_COMMAND_MOVE = 4
}enMCS_COMMAND;
#define MCS_COMMAND
#endif


