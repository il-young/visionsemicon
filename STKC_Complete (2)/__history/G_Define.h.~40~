	AnsiString SECS_T9;

	AnsiString SECS_COL;
	AnsiString SECS_ROW;

	AnsiString SECS_DEV_NUM;
	AnsiString SECS_MODELNAME;
	AnsiString SECS_VERSION;

	AnsiString DB_DIR;
}stSECSInfo;
#define SECSInfo
#endif

